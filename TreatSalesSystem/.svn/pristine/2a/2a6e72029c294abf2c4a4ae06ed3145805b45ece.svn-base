package app;

public abstract class Topper extends Treat {

    //EFFECTS:  ensures that the connecting phrase will be " on " if the object is onTopOf a Treat.
    public Topper(Treat onTopOf){
        connectingPhrase=" on ";
        this.onTopOf=onTopOf;
    }

    //EFFECTS:  ensures that the connecting phrase will be " and " if the object is onTopOf another Topper.
    public Topper(Topper onTopOf){
        connectingPhrase=" and ";
        this.onTopOf=onTopOf;
    }

}
